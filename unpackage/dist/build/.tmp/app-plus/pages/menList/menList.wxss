.cu-item {
  border-bottom: 1rpx solid #f2f2f2;
}
.mask-del {
  width: 100%;
  height: 100vh;
  position: fixed;
  left: 0;
  bottom: 0;
  background: rgba(1, 1, 1, 0.5);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.mask-del .mskeContent-center {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    background: #FFFFFF;
    width: 80%;
    height: 100rpx;
    border-radius: 20rpx;
    font-size: 34rpx;
    font-weight: bold;
    font-family: "Microsoft YaHei", 微软雅黑, "MicrosoftJhengHei", 华文细黑, STHeiti, MingLiu;
}
.indexBar {
  position: fixed;
  right: 0px;
  bottom: 10rpx;
  padding: 20rpx 20rpx 20rpx 60rpx;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.headurl {
  height: 90rpx;
  width: 90rpx;
  border-radius: 4px;
  /* margin-right: 20upx; */
}
.indexBar .indexBar-box {
  width: 40rpx;
  height: auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  border-radius: 10rpx;
}
.indexBar-item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  width: 40rpx;
  height: 25rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 20rpx !important;
}
movable-view.indexBar-item {
  width: 40rpx;
  height: 25rpx;
  z-index: 9;
  position: relative;
}
movable-view.indexBar-item::before {
  content: "";
  display: block;
  position: absolute;
  left: 0;
  top: 1rpx;
  height: 25rpx;
  width: 4rpx;
  background-color: #f37b1d;
}
.indexToast {
  position: fixed;
  top: 0;
  right: 80rpx;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
  margin: auto;
  color: #fff;
  line-height: 100rpx;
  text-align: center;
  font-size: 48rpx;
}
.text-grey {
  color: #000000;
  font-size: 34rpx;
  font-family: "Microsoft YaHei", 微软雅黑, "MicrosoftJhengHei", 华文细黑, STHeiti, MingLiu;
}

